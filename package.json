{
  "name": "structo-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*",
    "structopkgs/*",
    "structopkgs/airtable",
    "packages/query",
    "packages/host",
    "packages/cli",
    "packages/prepass",
    "packages/structo",
    "packages/react-ssr-prepass",
    "packages/data-sources-context",
    "packages/data-sources",
    "packages/react-web"
  ]
}
